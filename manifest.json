{
  "manifest_version": 3,
  "name": "YouTube Speed Toggle",
  "version": "1.0",
  "description": "Easily change playback speed on YouTube videos.",
  "icons": {
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_end"
    }
  ],
  "permissions": []
}
